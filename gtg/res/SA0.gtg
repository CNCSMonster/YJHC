BLOCK_IN,VAR,Nop
BLOCK_IN,FUNC,Nop
BLOCK_IN,RETURN,Nop
BLOCK_IN,CONTINUE,Nop
BLOCK_IN,BREAK,Nop
BLOCK_IN,OP,Nop
BLOCK_IN,CONST_KEYWORD,Nop

IF_BLOCK_IN,TYPE,Nop
IF_BLOCK_IN,VAR,Nop
IF_BLOCK_IN,FUNC,Nop
IF_BLOCK_IN,CONTINUE,Nop
IF_BLOCK_IN,RETURN,Nop
IF_BLOCK_IN,BREAK,Nop
IF_BLOCK_IN,OP,Nop
IF_BLOCK_IN,CONST_KEYWORD,Nop


ELIF_BLOCK_IN,TYPE,Nop
ELIF_BLOCK_IN,VAR,Nop
ELIF_BLOCK_IN,FUNC,Nop
ELIF_BLOCK_IN,CONTINUE,Nop
ELIF_BLOCK_IN,RETURN,Nop
ELIF_BLOCK_IN,BREAK,Nop
ELIF_BLOCK_IN,OP,Nop
ELIF_BLOCK_IN,CONST_KEYWORD,Nop

ELSE_BLOCK_IN,TYPE,Nop
ELSE_BLOCK_IN,VAR,Nop
ELSE_BLOCK_IN,FUNC,Nop
ELSE_BLOCK_IN,CONTINUE,Nop
