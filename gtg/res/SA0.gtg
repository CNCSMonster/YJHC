WHILE_BLOCK_IN,RETURN,Nop
WHILE_BLOCK_IN,BREAK,Nop
WHILE_BLOCK_IN,OP,Nop
WHILE_BLOCK_IN,CONST_KEYWORD,Nop

DO_WHILE_BLOCK_IN,TYPE,Nop
DO_WHILE_BLOCK_IN,VAR,Nop
DO_WHILE_BLOCK_IN,FUNC,Nop
DO_WHILE_BLOCK_IN,CONTINUE,Nop
DO_WHILE_BLOCK_IN,RETURN,Nop
DO_WHILE_BLOCK_IN,BREAK,Nop
DO_WHILE_BLOCK_IN,OP,Nop
