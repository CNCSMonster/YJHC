//支持面向对象编程
struct myData{
    int len;
    int getLen(int,int);   //结构体的成员默认是可以访问的   //结构体内部可以声明方程
}

//在结构体外面完成其方法定义
myData->int getLen(int a,int b){
    return (a-b)*len;
}



